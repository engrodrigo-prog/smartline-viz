{
  "version": 2,
  "routes": [
    {
      "src": "^/logo-smartline\\.png$",
      "headers": {
        "Cache-Control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "^/smartline-og\\.png$",
      "headers": {
        "Cache-Control": "public,max-age=604800"
      },
      "continue": true
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/[...path]?path=$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/((?!api/|@vite/|@react-refresh|__vite_ping|src/|node_modules/|assets/|public/|dist/|_next/|\\.well-known/|.*\\.[\\w]+$).*)",
      "dest": "/index.html"
    }
  ]
}
