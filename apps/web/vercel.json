{
  "version": 2,
  "routes": [
    {
      "src": "^/api/(.*)$",
      "dest": "/api/[...path]?path=$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/(.*)$",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/logo-smartline.png",
      "headers": [
        { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" }
      ]
    },
    {
      "source": "/smartline-og.png",
      "headers": [
        { "key": "Cache-Control", "value": "public,max-age=604800" }
      ]
    }
  ]
}
