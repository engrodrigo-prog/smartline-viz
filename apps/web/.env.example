VITE_API_BASE_URL=http://localhost:8080
# Nome exibido no app
VITE_APP_NAME=SmartLine
# Se true, usa dados demo e ignora auth
VITE_DEMO_MODE=false
VITE_DEMO_BYPASS_AUTH=false
VITE_CONTACT_EMAIL=guilherme@gpcad.com.br
# Ativa leitura das tabelas Supabase `sensors`, `sensor_readings` e `cameras`.
# Se false, usa catálogo/fallback local e evita 404 quando as tabelas não existem.
VITE_ENABLE_SENSOR_DB=false
# Supabase (front-end)
# Use a ANON key (recomendado) ou a Publishable key (se aplicável ao seu projeto).
# Este repo está linkado ao projeto ref `gvnxcwonodawrxeysjfl` (ver `supabase/config.toml`).
# Você pode obter as chaves com:
#   supabase projects api-keys --project-ref gvnxcwonodawrxeysjfl
VITE_SUPABASE_URL=https://gvnxcwonodawrxeysjfl.supabase.co
VITE_SUPABASE_ANON_KEY=ey...
# VITE_SUPABASE_PUBLISHABLE_KEY=ey...
# VITE_WIND_HEIGHT=100

# Backend (Vercel Functions /api) — definir no ambiente do servidor (NÃO usar VITE_)
